{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "%DB_CONNECTION_STRING%"
    },
    "Jwt": {
        "Key": "%JWT_SECRET_KEY%",
        "Issuer": "BlogApp",
        "Audience": "BlogAppUsers",
        "ExpireMinutes": 60
    }

}
